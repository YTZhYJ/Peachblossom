<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.pytd.db.entity">
	<class name="AuthUser" table="Auth_User">
		<id column="id" name="id"/>
  		<property column="name" generated="never" lazy="false" name="name" type="string"/>
  		<property column="password" generated="never" lazy="false" name="password" type="int"/>
 	</class>
</hibernate-mapping>
